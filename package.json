{
  "name": "js-sorted-set",
  "version": "0.7.0",
  "description": "Sorted set data structures",
  "main": "./lib/SortedSet.js",
  "browser": "./sorted-set.js",
  "scripts": {
    "test": "npm run build-cjs && mocha",
    "build-cjs": "babel ./src -d ./lib",
    "build-umd": "rollup --config",
    "build": "npm run build-cjs && npm run build-umd",
    "publish": "npm run build-cjs && mocha && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamhooper/js-sorted-set"
  },
  "keywords": [
    "sort",
    "sorted",
    "set",
    "sets",
    "left-leaning",
    "red-black",
    "binary",
    "tree",
    "iterate",
    "iterator",
    "comparator"
  ],
  "author": "Adam Hooper <adam@adamhooper.com>",
  "license": "Public Domain",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^6.2.2",
    "mocha": "^11.7.5",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "sinon": "^21.0.1",
    "sinon-chai": "^4.0.1"
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "cjs",
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
